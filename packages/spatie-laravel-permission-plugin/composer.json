{
    "name": "filament/spatie-laravel-permission-plugin",
    "description": "Filament support for `spatie/laravel-permission`.",
    "license": "MIT",
    "homepage": "https://github.com/laravel-filament/filament",
    "support": {
        "issues": "https://github.com/laravel-filament/filament/issues",
        "source": "https://github.com/laravel-filament/filament"
    },
    "require": {
        "php": "^8.0",
        "filament/filament": "self.version",
        "spatie/laravel-permission": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Filament\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\SpatieLaravelPermissionPluginServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
