{
    "name": "filament/jetstream-extension",
    "description": "Extending Laravel Jetstream - integrated with Filament",
    "license": "MIT",
    "homepage": "https://github.com/laravel-filament/filament",
    "support": {
        "issues": "https://github.com/laravel-filament/filament/issues",
        "source": "https://github.com/laravel-filament/filament"
    },
    "require": {
        "php": "^7.4|^8.0",
        "blade-ui-kit/blade-heroicons": "^0.3",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0",
        "livewire/livewire": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Filament\\JetstreamExtension\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\JetstreamExtension\\JetstreamExtensionServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
